{
  "name": "Fisheye GL",
  "description": "Correct fisheye, or barrel distortion, in images (with WebGL -- adapted from fisheye-correction-webgl by @bluemir).",
  "requires": [ "webgl" ],
  "inputs": {
    "a": {
      "type": "float",
      "desc": "a parameter",
      "htmlType":
      {
        "type": "range",
        "default": 1,
        "min": 1,
        "max": 4,
        "step": 0.2
      }
    },
    "b": {
      "type": "float",
      "desc": "b parameter",
      "htmlType":
      {
        "type": "range",
        "default": 1,
        "min": 1,
        "max": 4,
        "step": 0.2
      }
    },
    "Fx": {
      "type": "float",
      "desc": "Fx parameter",
      "htmlType":
      {
        "type": "range",
        "default": 1,
        "min": 1,
        "max": 4,
        "step": 0.2
      }
    },
    "Fy": {
      "type": "float",
      "desc": "Fy parameter",
      "htmlType":
      {
        "type": "range",
        "default": 1,
        "min": 1,
        "max": 4,
        "step": 0.2
      }
    },
    "scale": {
      "type": "float",
      "desc": "Image Scaling",
      "htmlType":
      {
        "type": "range",
        "default": 1.5,
        "min": 0,
        "max": 20,
        "step": 0.5
      }
    },
    "x": {
      "type": "float",
      "desc": "FOV x parameter",
      "htmlType":
      {
        "type": "range",
        "default": 1.5,
        "min": 0,
        "max": 20,
        "step": 0.5
      }
    },
    "y": {
      "type": "float",
      "desc": "FOV y parameter",
      "htmlType":
      {
        "type": "range",
        "default": 1.5,
        "min": 0,
        "max": 20,
        "step": 0.5
      }
    },
    "fragmentSrc": {
      "type": "PATH",
      "desc": "Path to a WebGL fragment shader file",
      "htmlType":
      {
        "type": "text",
        "default": "(inbuilt)"
      }
    },
    "vertexSrc": {
      "type": "PATH",
      "desc": "Path to a WebGL vertex shader file",
      "htmlType":
      {
        "type": "text",
        "default": "(inbuilt)"
      }
    }
  },
  "docs-link":"https://github.com/publiclab/image-sequencer/blob/main/docs/MODULES.md"
}
